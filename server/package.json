{
  "name": "e-commerce-for-ittrend",
  "version": "1.0.0",
  "description": "e-commerce-for-ittrend",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "dev": "SET NODE_ENV=development& nodemon server.js",
    "prod": "SET NODE_ENV=production& server.js",
    "make:migrations": "npx knex migrate:make init --migrations-directory migrations",
    "migrate:latest": "npx knex migrate:latest --knexfile knexfile.js",
    "seed:run": "npx knex seed:run",
    "migrate:rls": "npx knex migrate:rollback && knex migrate:latest --knexfile knexfile.js && npx knex seed:run"
  },
  "author": "Emman Jay Dizon",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cloudinary": "^1.29.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "g": "^2.0.1",
    "init": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.7",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
